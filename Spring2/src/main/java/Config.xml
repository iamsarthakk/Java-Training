<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id = "id1" class="com.autowire.Categories" autowire="byName">
		<property name="name" value="General Books"/>
	</bean>
	<bean id="bk" class="com.autowire.Book">
		<property name="bookname" value="Spring reference"/>
		<property name="bookprice" value="300"/>
	</bean>
</beans>