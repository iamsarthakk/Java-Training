
public class Emp {
	int eid;
	String ename;
	double esal;
	//Emp this;
	
	public Emp() {
		eid = 0;
		ename = " ";
		esal = 0.00;
	}
	
	public Emp(int eid, String ename, double esal) {
		this.eid = eid;
		this.ename = ename;
		this.esal = esal;
	}
	public void input() {
		eid = 101;
		ename = "Sarthak";
		esal = 200000.00;
	}
	public void output() {
		System.out.println(eid+" "+ename+" "+esal);
	}
}
