
public class Test3 {

	public static void main(String[] args) {
		
		//Arrays in Java
		int arr[];     			 //step 1
		arr = new int[4];   //step2
		arr[0] = 25;
		arr[1] = 35;
		arr[2] = 15;
		arr[3] = 5;
		
		String sarr[];
		sarr = new String[5];
		sarr[0]="Sarthak";
		sarr[1]="Abhigyan";
		sarr[2]="Riaz";
		sarr[3]="Ankit";
		sarr[4]="Danveer";
		
		for(int i=0;i<5;i++) {
			System.out.println(sarr[i]);
		}
		
		
		
		//Enhanced For Loop
		for(int i : arr) {
			System.out.println(i);
		}
	}

}
